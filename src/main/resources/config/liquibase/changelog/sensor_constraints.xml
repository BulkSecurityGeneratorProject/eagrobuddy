<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Sensor.
    -->
    <changeSet id="20180422181228-2" author="Rajee">
        
      <addForeignKeyConstraint baseColumnNames="sensor_id"
                                 baseTableName="sensor"
                                 constraintName="fk_sensor_sensor_coverage_range_id"
                                 referencedColumnNames="sensor_id"
                                 referencedTableName="sensor_coverage_range"/>

        <addForeignKeyConstraint baseColumnNames="sensor_id"
                                 baseTableName="sensor"
                                 constraintName="fk_sensor_section_sensor_mapping_id"
                                 referencedColumnNames="sensor_id"
                                 referencedTableName="section_sensor_mapping"/>

        <addForeignKeyConstraint baseColumnNames="sensor_id"
                                 baseTableName="sensor"
                                 constraintName="fk_sensor_sensor_data_id"
                                 referencedColumnNames="sensor_id"
                                 referencedTableName="sensor_data"/>

        <addForeignKeyConstraint baseColumnNames="layout_id"
                                 baseTableName="sensor"
                                 constraintName="fk_sensor_layout_id"
                                 referencedColumnNames="layout_id"
                                 referencedTableName="layout"/>

    </changeSet>
</databaseChangeLog>
